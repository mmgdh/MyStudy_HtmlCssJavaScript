
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Html5_SVG</title>
</head>
<body>
    <svg width="100%" height="100%">
        <circle cx="300" cy="100" r="50" stroke="#FF0" stroke-width="3" fill="green"></circle>
    </svg>
    
    <svg width="300px" height="150">
        <rect x="20" y="30" width="250px" height="125px" rx="5" ry="5" fill="teal"></rect>
    </svg>

    <svg width="300px" height="300px">
        <ellipse cx="150" cy="150" rx="100" ry="75" fill="blue"> </ellipse>
    </svg>

    <svg width="300px" height="250px">
        <line x1="100" y1="200" x2="250" y2="50" stroke="#000" stroke-width="5"></line>
    </svg>

    <svg width="300px" height="300px">
        <polyline points="10 10,50 50,75 175,175 150,175 50,225 75,225 150,300 150" fill="none" stroke="#000"></polyline>
    </svg>

    <svg width="300px" height="200px">
        <polygon points="10 10,50 50,75 175,175 150,175 50,225 75,225 150,300 150" fill="red" stroke="#000"></polygon>
    </svg>
</body>
</html>
